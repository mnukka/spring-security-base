<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ee.company.crm.domain.persistence.sector.SectorDao">
    <select id="findById" parameterType="java.lang.Long" resultType="ee.company.crm.domain.persistence.user.profile.UserSectorEntity">
        SELECT * from user_sector where id = #{id} AND enabled = 'true';
    </select>

</mapper>
