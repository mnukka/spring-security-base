<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ee.company.crm.domain.persistence.user.UserDao">
    <select id="findByUsername" parameterType="java.lang.String" resultType="ee.company.crm.domain.persistence.user.UserEntity">
        SELECT * from user where username = #{username}
    </select>
</mapper>
